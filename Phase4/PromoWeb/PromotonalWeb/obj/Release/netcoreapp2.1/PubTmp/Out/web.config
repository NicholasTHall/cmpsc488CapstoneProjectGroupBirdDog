      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\PromotonalWeb.dll" stdoutLogEnabled="false" stdoutLogFile="\\?\%home%\LogFiles\stdout" />
    </system.webServer>
  </location>
</configuration>
